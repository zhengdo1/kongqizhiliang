<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tool="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!--<View-->
        <!--android:id="@+id/paddingLine"-->
        <!--style="@style/allListPaddingLine"/>-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0px"
                android:layout_height="120dp"
                android:layout_marginRight="30dp"
                android:layout_weight="3"
                android:orientation="vertical"
                android:paddingBottom="@dimen/activity_bottom_margin"
                android:paddingTop="@dimen/activity_top_margin">



                <include
                    android:id="@+id/layout_itemPortList_outputcode"
                    layout="@layout/layout_left_question_right_answer2" />

                <include
                    android:id="@+id/layout_itemPortList_outputname"
                    layout="@layout/layout_left_question_right_answer2" />

                <include
                    android:id="@+id/layout_itemPortList_mn"
                    layout="@layout/layout_left_question_right_answer2" />

                <include
                    android:id="@+id/layout_itemPortList_outputtype"
                    layout="@layout/layout_left_question_right_answer2" />

                <include
                    android:id="@+id/layout_itemPortList_outputpointtype"
                    layout="@layout/layout_left_question_right_answer2" />

                <include
                    android:id="@+id/layout_itemPortList_ifsintering"
                    layout="@layout/layout_left_question_right_answer2" />
            </LinearLayout>

            <ImageView
                style="@style/portListRightArrowStyle"
                android:background="@mipmap/ic_rightarrow" />
        </LinearLayout>
        <!--<View-->
        <!--android:id="@+id/list_bottom_line"-->
        <!--android:visibility="gone"-->
        <!--style="@style/allLines"-->
        <!--/>-->
    </LinearLayout>


</LinearLayout>
