<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_itemData_time"
        android:layout_width="@dimen/data_time"
        android:layout_height="wrap_content"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        android:text="Type"
        />

    <vb.shlv.InterceptScrollContainer
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="false"
        >

        <vb.shlv.MyHScrollView
            android:id="@+id/list_scroll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="false"
            android:scrollbars="none"
            >

            <LinearLayout
                android:id="@+id/ll_itemData_data"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:focusable="false"
                android:divider="@drawable/ll_devider"
                android:showDividers="middle"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="2dp"
                android:paddingRight="2dp">



            </LinearLayout>

        </vb.shlv.MyHScrollView>

    </vb.shlv.InterceptScrollContainer>

</LinearLayout>